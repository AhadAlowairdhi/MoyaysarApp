<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.MoyaysarApp" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/moyasargreen</item>
        <item name="colorPrimaryVariant">@color/moyasarblue</item>
        <item name="colorOnPrimary">@color/moyasargray</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/moyasarnavy</item>
        <item name="colorSecondaryVariant">@color/moyasarlightblue</item>
        <item name="colorOnSecondary">@color/moyasargray</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>
</resources>